#include<bits/stdc++.h>
class Solution {   // TC: O(n) | SC: O(n) | BRUTEFORCE
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        int ind = 0;
        vector<int>ans(n,0);

        for(int i=0; i<n; i++){
            if(nums[i]!=0)ans[ind++] = nums[i];
        }

        for(int i=0; i<n; i++){
           nums[i] = ans[i];
        }
    }
};




class Solution { // TC: O(n) |  SC: O(1)  OPTIMAL
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        int cnt = 0;
        int ind = 0;

        for(int i=0; i<n; i++){
            if(nums[i] == 0)cnt++;
            else nums[ind++] = nums[i];
        }

        int j = n-1;

        for(int i=0; i<cnt; i++){
            nums[j--] = 0;;
        }
    }
};
